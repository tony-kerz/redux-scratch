{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "exclude": [
      "app/api/new-york-times.js",
      "app/api/patients.js",
      "app/session/session.js"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  }
}
